		"react-table": "^7.6.1",
		"react-tabs": "^3.1.1",
		"react-toastify": "^6.0.9",
		"react-tooltip": "^4.2.10",
		"react-use-dimensions": "^1.2.1",
		"recoil": "^0.0.13",
		"use-debounce": "^5.0.1",
		"use-deep-compare-effect": "^1.4.0"
	}
